{
  "publisher": "hp",
  "name": "vscode-ext-prettier-java",
  "displayName": "Seamless Prettier For Java 2022",
  "description": "VSCode extension enabling *fast* format-on-save for Java sources using prettier=^2.7 and prettier-plugin-java=^1.6.",
  "version": "0.1.5",
  "engines": {
    "vscode": "^1.45.0"
  },
  "categories": [
    "Formatters"
  ],
  "keywords": [
    "prettier",
    "Java",
    "formatter"
  ],
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/thomas-schweich/vscode-ext-prettier-java"
  },
  "icon": "prettier-avatar-dark.png",
  "activationEvents": [
    "onLanguage:java"
  ],
  "contributes": {
    "languages": [
      {
        "id": "java",
        "aliases": [
          "Java"
        ],
        "extensions": [
          "java"
        ]
      }
    ]
  },
  "license": "MIT",
  "scripts": {
    "vscode:prepublish": "npm run esbuild-base -- --minify",
    "esbuild-base": "esbuild ./src/extension.ts --bundle --outfile=out/main.js --external:vscode --format=cjs --platform=node",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "test-compile": "tsc -p ./"
  },
  "dependencies": {
    "lodash": ">=4.17.19",
    "prettier": "^2.7.1",
    "prettier-plugin-java": "^1.6.2"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.34.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "esbuild": "^0.14.53",
    "eslint": "^8.13.0",
    "typescript": "^4.7.2"
  }
}
